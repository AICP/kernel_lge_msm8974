&soc {
	i2c@f9923000 {
		status = "ok";
		qcom,active-only;
		qcom,i2c-src-freq = <50000000>;
		qcom,master-id = <86>;

		max17048@36 {
			status =  "ok";
			interrupt-parent = <&pm8941_gpios>;
			interrupts = <20 0x0>;
			max17048,alert_gpio = <&pm8941_gpios 20 0x00>;
			max17048,alert_threshold = <2>;
			max17048,rcomp = <64>;
			max17048,temp_co_hot = <338>;
			max17048,temp_co_cold = <4338>;
			max17048,full_design = <2900>;
			max17048,empty = <0>;
		};
		bq24192@6b {
			status = "disable";
			revision = "...";
		};
	};
	qcom,msm-thermal {
		qcom,limit-temp = <80>;
		qcom,temp-hysteresis = <5>;
		qcom,core-limit-temp = <90>;
		qcom,core-temp-hysteresis = <5>;
	};
	pm8941,lge_boost_gpio {
		compatible = "pm8941,lge_boost_gpio";
		label = "PM8941 GPIO 21 for Boost IC";
		status = "ok";
		pm8941,bb_boost_gpio_name = "bb_boost_gpio";
		pm8941,bb_gpio = <&pm8941_gpios 21 0>;
	};
};

&spmi_bus {
	qcom,pm8941@0 {
		pm8941_chg: qcom,charger {
			status = "ok";
			qcom,ibatmax-dcp-ma = <1600>;
			qcom,ibatmax-qc20-ma = <2100>;
			qcom,ibatsafe-ma = <2200>;
			qcom,btc-disabled;

			qcom,chgr@1000 {
				status = "ok";
			};

			qcom,buck@1100 {
				status = "ok";
			};

			qcom,bat-if@1200 {
				status = "ok";
			};

			pm8941_chg_otg: qcom,usb-chgpth@1300 {
				status = "ok";
				regulator-name = "8941_smbb_otg";
			};

			qcom,dc-chgpth@1400 {
				status = "disabled";
			};

			pm8941_chg_boost: qcom,boost@1500 {
				status = "ok";
			};

			qcom,chg-misc@1600 {
				status = "ok";
			};

		};

		vadc@3100 {
			chan@13 {
				label = "pcb_rev";
				reg = <0x13>;
				qcom,decimation = <0>;
				qcom,pre-div-channel-scaling = <0>;
				qcom,calibration-type = "absolute";
				qcom,scale-function = <0>;
				qcom,hw-settle-time = <0>;
				qcom,fast-avg-setup = <0>;
			};

			chan@14 {
				status = "ok";
				label = "lcd_maker_id";
				reg = <0x14>;
				qcom,decimation = <0>;
				qcom,pre-div-channel-scaling = <0>;
				qcom,calibration-type = "absolute";
				qcom,scale-function = <0>;
				qcom,hw-settle-time = <0>;
				qcom,fast-avg-setup = <0>;
			};
			chan@15 {
				status = "ok";
				label="mpp6_adc";
				reg = <0x15>;
				qcom,decimation = <0>;
				qcom,pre-div-channel-scaling = <0>;
				qcom,calibration-type = "absolute";
				qcom,scale-function = <0>;
				qcom,hw-settle-time = <0>;
				qcom,fast-avg-setup = <0>;
			};
			chan@33 {
				label = "smb349_vchg";
				reg = <0x33>;
				qcom,decimation = <0>;
				qcom,pre-div-channel-scaling = <0>;
				qcom,calibration-type = "absolute";
				qcom,scale-function = <0>;
				qcom,hw-settle-time = <0>;
				qcom,fast-avg-setup = <0>;
				status = "disable";
			};

			chan@39 {
				label = "usb_id_lv";
				reg = <0x39>;
				qcom,decimation = <0>;
				qcom,pre-div-channel-scaling = <0>;
				qcom,calibration-type = "absolute";
				qcom,scale-function = <0>;
				qcom,hw-settle-time = <0>;
				qcom,fast-avg-setup = <0>;
			};
		};

		pm8941_bms: qcom,bms  {
			status = "ok";
			qcom,use-external-rsense;
		};

		qcom,currentnow@3800 {
			status = "ok";
		};
	};

	qcom,pm8941@1 {
