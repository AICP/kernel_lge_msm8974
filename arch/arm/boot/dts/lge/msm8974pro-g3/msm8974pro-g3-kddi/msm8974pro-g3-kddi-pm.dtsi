		};

		vadc@3100 {
			chan@13 {
				label = "pcb_rev";
				reg = <0x13>;
				qcom,decimation = <0>;
				qcom,pre-div-channel-scaling = <0>;
				qcom,calibration-type = "absolute";
				qcom,scale-function = <0>;
				qcom,hw-settle-time = <0>;
				qcom,fast-avg-setup = <0>;
			};

			chan@14 {
				status = "ok";
				label = "lcd_maker_id";
				reg = <0x14>;
				qcom,decimation = <0>;
				qcom,pre-div-channel-scaling = <0>;
				qcom,calibration-type = "absolute";
				qcom,scale-function = <0>;
				qcom,hw-settle-time = <0>;
				qcom,fast-avg-setup = <0>;
			};
			chan@15 {
				status = "ok";
				label="mpp6_adc";
				reg = <0x15>;
				qcom,decimation = <0>;
				qcom,pre-div-channel-scaling = <0>;
				qcom,calibration-type = "absolute";
				qcom,scale-function = <0>;
				qcom,hw-settle-time = <0>;
				qcom,fast-avg-setup = <0>;
			};
			chan@33 {
				label = "smb349_vchg";
				reg = <0x33>;
				qcom,decimation = <0>;
				qcom,pre-div-channel-scaling = <0>;
				qcom,calibration-type = "absolute";
				qcom,scale-function = <0>;
				qcom,hw-settle-time = <0>;
				qcom,fast-avg-setup = <0>;
				status = "disable";
			};

			chan@39 {
				label = "usb_id_lv";
				reg = <0x39>;
				qcom,decimation = <0>;
				qcom,pre-div-channel-scaling = <0>;
				qcom,calibration-type = "absolute";
				qcom,scale-function = <0>;
				qcom,hw-settle-time = <0>;
				qcom,fast-avg-setup = <0>;
			};

			chan@b5 {
				lge,rev-check;
				rev_b {
					revision = "...rev_b";
					sub_reg = <0xb7>;
				};

				rev_1{
					revision = "rev_c...";
					sub_reg = <0xb5>;
				};
			};

			chan@b7 {
				lge,rev-check;
				rev_b {
					revision = "...rev_b";
					sub_reg = <0xb8>;
				};

				rev_1 {
					revision = "rev_c...";
					sub_reg = <0xb7>;
				};
			};
		};

		bif_ctrl: qcom,bsi@1b00 {
			status = "disable";
		};

		pm8941_bms: qcom,bms  {
			status = "ok";
			qcom,use-external-rsense;
		};

		qcom,currentnow@3800 {
