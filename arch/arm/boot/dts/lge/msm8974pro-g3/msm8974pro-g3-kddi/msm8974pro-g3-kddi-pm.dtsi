			lge,cable-28p7k = <300000 1500 500>;
			lge,cable-56k = <490000 1500 1500>;
			lge,cable-100k = <650000 1500 500>;
			lge,cable-130k = <780000 500 500>;
			lge,cable-180k = <875000 1500 500>;
